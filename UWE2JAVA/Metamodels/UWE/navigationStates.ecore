    <eStructuralFeatures xsi:type="ecore:EAttribute" name="isHome" ordered="false"
        eType="ecore:EDataType types.ecore#//Boolean">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="&lt;html>&#xA;  &lt;head>&#xA;&#x9;&#x9;&lt;style>&#xA;&#x9;&#x9;&#x9;p {padding:0px; margin:0px;}&#xA;&#x9;&#x9;&lt;/style>&#xA;&#x9;&lt;/head>&#xA;&#xA;  &lt;body>&#xA;    &lt;p>&#xA;      &lt;b>&lt;font size=&quot;2&quot;>&amp;#171;navigationalNode&amp;#187; {isHome}&lt;/font>&lt;/b>&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;      &lt;font size=&quot;2&quot;>s&lt;/font>&lt;font size=&quot;2&quot;>pecifies that this is the main &#xA;      node the users go to when accessing the &lt;/font>&lt;font size=&quot;2&quot;>web &lt;/font>&lt;font size=&quot;2&quot;>application. &#xA;      &lt;/font>&lt;font size=&quot;2&quot;>Usually&lt;/font>&lt;font size=&quot;2&quot;> it is&lt;/font>&lt;font size=&quot;2&quot;> &#xA;      related &lt;/font>&lt;font size=&quot;2&quot;>to&lt;/font>&lt;font size=&quot;2&quot;> the home page of &#xA;      the &lt;/font>&lt;font size=&quot;2&quot;>whole &lt;/font>&lt;font size=&quot;2&quot;>application.&lt;/font>&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;      &#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;      &lt;b>&lt;font size=&quot;2&quot;>Constraint&lt;/font>&lt;/b>&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;      &lt;font size=&quot;2&quot;>Exactly one &amp;#171;navigationalNode&amp;#187; has the {isHome=true} tag &lt;/font>&lt;font size=&quot;2&quot;>with&lt;/font>&lt;font size=&quot;2&quot;>in &#xA;      one&lt;/font>&lt;font size=&quot;2&quot;> web&lt;/font>&lt;font size=&quot;2&quot;> application.&lt;/font>&#xA;    &lt;/p>&#xA;&#xA;  &lt;/body>&#xA;&#xA;&lt;/html>&#xA;&#xA;"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="navigationMenu" ordered="false"
        lowerBound="1" upperBound="-1" eType="#//NavigationMenu"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="isModal" ordered="false"
        lowerBound="1" eType="ecore:EDataType types.ecore#//Boolean">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="&lt;html>&#xA;  &lt;head>&#xA;&#x9;&#x9;&lt;style>&#xA;&#x9;&#x9;&#x9;p {padding:0px; margin:0px;}&#xA;&#x9;&#x9;&lt;/style>&#xA;&#x9;&lt;/head>&#xA;&#xA;  &lt;body>&#xA;    &lt;p>&#xA;      &lt;font size=&quot;2&quot;>s. &lt;b>isModal&lt;/b> &lt;/font>&lt;font size=&quot;2&quot;>specification&lt;/font>&lt;font size=&quot;2&quot;> &#xA;      and abbreviation example&lt;/font>&#xA;    &lt;/p>&#xA;&#xA;  &lt;/body>&#xA;&#xA;&lt;/html>&#xA;&#xA;&#xD;&#xA;&lt;html>&#xA;  &lt;head>&#xA;&#x9;&#x9;&lt;style>&#xA;&#x9;&#x9;&#x9;p {padding:0px; margin:0px;}&#xA;&#x9;&#x9;&lt;/style>&#xA;&#x9;&lt;/head>&#xA;&#xA;  &lt;body>&#xA;    &lt;p>&#xA;      &lt;font size=&quot;2&quot;>s. &lt;b>isModal&lt;/b> &lt;/font>&lt;font size=&quot;2&quot;>specification&lt;/font>&lt;font size=&quot;2&quot;> &#xA;      and abbreviation example&lt;/font>&#xA;    &lt;/p>&#xA;&#xA;  &lt;/body>&#xA;&#xA;&lt;/html>&#xA;&#xA;&#xD;&#xA;&lt;html>&#xA;  &lt;head>&#xA;&#x9;&#x9;&lt;style>&#xA;&#x9;&#x9;&#x9;p {padding:0px; margin:0px;}&#xA;&#x9;&#x9;&lt;/style>&#xA;&#x9;&lt;/head>&#xA;&#xA;  &lt;body>&#xA;    &lt;p>&#xA;      &lt;b>&amp;#171;NavigationalNode&amp;#187; {isModal=true}&lt;/b>&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;      guarantees that only this node is active and that it can only be left by &#xA;      a transition that goes back to the previously active state. All parallel &#xA;      regions are restored after having left the modal state. This represents &#xA;      the navigational behavior of a modal dialog.&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;      &#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;      &lt;b>Constraints&lt;/b>&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;      1 Exactly one transition targets the modal state, with a unique action., &#xA;      i.e. an action that cannot occur elsewhere at the same time.&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;      2 Exactly one transition goes back from the modal state to the previous &#xA;      state.&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;      3 These two transitions are the only ones that are connected with the &#xA;      modal state.&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;      &#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;      &lt;b>Definition &lt;/b>&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;      A new composite state z is added that includes the outermost state &#xA;      machine of the web application, i.e. the state with the tag &#xA;      {isHome=true} and connected states.&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;      A new initial state is added, which is connected with z.&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;      The modal state m is moved outside of z and the transition that comes &#xA;      from m targets a new history state in z, instead of the state before m.&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;      For transitions that cross state machine borders, entry and exit points &#xA;      are used.&#xA;    &lt;/p>&#xA;&#xA;  &lt;/body>&#xA;&#xA;&lt;/html>&#xA;&#xA;"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="accessibleInAppModes" ordered="false"
        lowerBound="1" upperBound="-1" eType="ecore:EClass applicationMode.ecore#//AppMode"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="notAccessibleInAppModes"
        ordered="false" lowerBound="1" upperBound="-1" eType="ecore:EClass applicationMode.ecore#//AppMode"/>
