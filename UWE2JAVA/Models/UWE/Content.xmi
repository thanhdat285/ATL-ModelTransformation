<?xml version="1.0" encoding="UTF-8"?>
<uml:Package
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:uml="http:///uml.ecore"
    xsi:schemaLocation="http:///uml.ecore ../../Metamodels/UWE/uml.ecore"
    xmi:id="content"
    name="content">
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="AddressBook"
      name="AddressBook">
    <ownedAttribute
        name="introduction"
        visibility="private"
        type="String"/>
    <ownedAttribute
        name="contacts"
        visibility="private"
        type="Contact"
        isOrdered="true"
        isDerived="true">
      <lowerValue
          xsi:type="uml:LiteralInteger"/>
      <upperValue
          xsi:type="uml:LiteralUnlimitedNatural"
          value="10"/>
    </ownedAttribute>
  </packagedElement>
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="Contact"
      name="Contact">
    <ownedAttribute
        name="name"
        visibility="private"
        type="String"/>
    <ownedAttribute
        name="email"
        visibility="private"
        type="String"/>
    <ownedAttribute
        name="addressBook"
        visibility="private"
        type="AddressBook"/>
    <ownedAttribute
        name="postalAddrMain"
        visibility="private"
        type="Address"/>
    <ownedAttribute
        name="postalAddrAlternative"
        visibility="private"
        type="Address"/>
    <ownedAttribute
        name="phoneMain"
        visibility="private"
        type="Phone"/>
    <ownedAttribute
        name="phoneAlternative"
        visibility="private"
        type="Phone"/>
    <ownedAttribute
        name="picture"
        visibility="private"
        type="Picture"/>
  </packagedElement>
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="Address"
      name="Address">
    <ownedAttribute
        name="name"
        visibility="private"
        type="String"/>
    <ownedAttribute
        name="postalCode"
        visibility="private"
        type="Integer"/>
    <ownedAttribute
        name="city"
        visibility="private"
        type="String"/>
    <ownedAttribute
        name="country"
        visibility="private"
        type="String"/>
  </packagedElement>
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="Phone"
      name="Phone">
    <ownedAttribute
        name="internationalAreaCode"
        visibility="private"
        type="Integer"/>
    <ownedAttribute
        name="prefix"
        visibility="private"
        type="Integer"/>
    <ownedAttribute
        name="number"
        visibility="private"
        type="Integer"/>
  </packagedElement>
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="Picture"
      name="Picture">
    <ownedAttribute
        name="width"
        visibility="private"
        type="Integer"/>
    <ownedAttribute
        name="height"
        visibility="private"
        type="Integer"/>
    <ownedAttribute
        name="contact"
        visibility="private"
        type="Contact"/>
  </packagedElement>
  <packagedElement
      xsi:type="uml:PrimitiveType"
      xmi:id="String"
      name="java.lang.String"/>
  <packagedElement
      xsi:type="uml:PrimitiveType"
      xmi:id="Integer"
      name="int"/>
</uml:Package>
