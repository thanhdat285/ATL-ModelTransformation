<?xml version="1.0" encoding="UTF-8"?>
<uml:Package
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:uml="http:///uml.ecore"
    xsi:schemaLocation="http:///uml.ecore ../../Metamodels/UWE/uml.ecore"
    xmi:id="addressbook"
    name="addressbook"
    URI="">
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="ContactProcessing"
      name="ContactProcessing"
      isAbstract="true">
    <ownedAttribute
        name="contactDataInput"
        visibility="private"
        type="ContactDataInput"/>
  </packagedElement>
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="Confirmation"
      name="Confirmation"
      isAbstract="true">
    <ownedAttribute
        name="question"
        visibility="private"
        type="String"/>
  </packagedElement>
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="ContactCreation"
      name="ContactCreation">
    <generalization
        general="ContactProcessing"
        isSubstitutable="false"/>
  </packagedElement>
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="SaveConfirmation"
      name="SaveConfirmation">
    <generalization
        general="Confirmation"
        isSubstitutable="false"/>
    <ownedAttribute
        name="contactCreation"
        visibility="private"
        type="ContactCreation"/>
    <ownedOperation
        name="saveContact"/>
  </packagedElement>
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="ContactUpdate"
      name="ContactUpdate">
    <generalization
        general="ContactProcessing"
        isSubstitutable="false"/>
  </packagedElement>
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="UpdateConfirmation"
      name="UpdateConfirmation">
    <generalization
        general="Confirmation"
        isSubstitutable="false"/>
    <ownedAttribute
        name="contactUpdate"
        visibility="private"
        type="ContactUpdate"/>
    <ownedOperation
        name="updateContact"/>
  </packagedElement>
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="ContactDeletion"
      name="ContactDeletion">
    <generalization
        general="ContactProcessing"
        isSubstitutable="false"/>
    <ownedAttribute
        name="contact"
        visibility="private"
        type="String"/>
  </packagedElement>
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="DeleteConfirmation"
      name="DeleteConfirmation">
    <generalization
        general="Confirmation"
        isSubstitutable="false"/>
    <ownedAttribute
        name="contactDeletion"
        visibility="private"
        type="ContactDeletion"/>
    <ownedOperation
        name="deleteContact"/>
  </packagedElement>
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="ContactDataInput"
      name="ContactDataInput"/>
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="ValidationError"
      name="ValidationError">
    <ownedAttribute
        name="message"
        visibility="private"
        type="String"/>
    <ownedAttribute
        name="contactDataInput"
        visibility="private"
        type="ContactDataInput"/>
  </packagedElement>
  <packagedElement
      xsi:type="uml:Class"
      name="Contact"
      isActive="true">
    <ownedOperation
        name="saveContact"/>
    <ownedOperation
        name="updateContact"/>
    <ownedOperation
        name="deleteContact"/>
    <ownedOperation
        name="validateData"/>
  </packagedElement>
  <packagedElement
      xsi:type="uml:PrimitiveType"
      xmi:id="String"
      name="java.lang.String"/>
</uml:Package>
