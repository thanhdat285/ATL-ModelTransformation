<?xml version="1.0" encoding="UTF-8"?>
<uml:Package
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:uml="http:///uml.ecore"
    xsi:schemaLocation="http:///uml.ecore ../../Metamodels/UWE/uml.ecore"
    xmi:id="controller"
    name="controller"
    URI="">
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="Category"
      name="Category">
    <ownedOperation
        name="categories">
      <ownedComment
          body="/categories"/>
    </ownedOperation>
    <ownedOperation
        name="category">
      <ownedComment
          body="/categories/{id:\d+}"/>
    </ownedOperation>
    <ownedOperation
        name="search">
      <ownedComment
          body="/categories/search"/>
    </ownedOperation>
    <ownedOperation
        name="items">
      <ownedComment
          body="/categories/{id:\d+}/items"/>
    </ownedOperation>
  </packagedElement>
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="Item"
      name="Item">
    <ownedOperation
        name="items">
      <ownedComment
          body="/items"/>
    </ownedOperation>
    <ownedOperation
        name="item">
      <ownedComment
          body="/items/&lt;id>"/>
    </ownedOperation>
    <ownedOperation
        name="search">
      <ownedComment
          body="/items/search"/>
    </ownedOperation>
  </packagedElement>
  <packagedElement
      xsi:type="uml:Class"
      xmi:id="Brand"
      name="Brand">
    <ownedOperation
        name="brands">
      <ownedComment
          body="/brands"/>
    </ownedOperation>
    <ownedOperation
        name="brand">
      <ownedComment
          body="/brands/&lt;id>"/>
    </ownedOperation>
    <ownedOperation
        name="search">
      <ownedComment
          body="/brands/search"/>
    </ownedOperation>
    <ownedOperation
        name="items">
      <ownedComment
          body="/brands/&lt;id>/items"/>
    </ownedOperation>
  </packagedElement>
</uml:Package>
