<?xml version="1.0" encoding="UTF-8"?>
<xmi:XMI xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:presentation="http:///presentation.ecore"
    xmlns:uml="http:///uml.ecore"
    xsi:schemaLocation="http:///presentation.ecore ../../Metamodels/UWE/presentation.ecore http:///uml.ecore ../../Metamodels/UWE/uml.ecore">
  <presentation:ProcessClass
      baseClass="SaveConfirmation"
      contentClass="Contact"/>
  <presentation:ProcessClass
      baseClass="UpdateConfirmation"
      contentClass="Contact"/>
  <presentation:ProcessClass
      baseClass="DeleteConfirmation"
      contentClass="Contact"/>
  <presentation:ProcessClass
      baseClass="ContactDataInput"
      contentClass="Contact"/>
  <uml:Package
      xmi:id="process"
      name="process">
    <packagedElement
        xsi:type="uml:Class"
        xmi:id="ContactProcessing"
        name="ContactProcessing"
        isAbstract="true">
      <ownedAttribute
          name="contactDataInput"
          visibility="private"
          type="ContactDataInput"/>
    </packagedElement>
    <packagedElement
        xsi:type="uml:Class"
        xmi:id="Confirmation"
        name="Confirmation"
        isAbstract="true">
      <ownedAttribute
          name="question"
          visibility="private"
          type="String"/>
    </packagedElement>
    <packagedElement
        xsi:type="uml:Class"
        xmi:id="ContactCreation"
        name="ContactCreation">
      <generalization
          general="ContactProcessing"
          isSubstitutable="false"/>
    </packagedElement>
    <packagedElement
        xsi:type="uml:Class"
        xmi:id="SaveConfirmation"
        name="SaveConfirmation">
      <generalization
          general="Confirmation"
          isSubstitutable="false"/>
      <ownedAttribute
          name="contactCreation"
          visibility="private"
          type="ContactCreation"/>
      <ownedOperation
          name="saveContact"/>
    </packagedElement>
    <packagedElement
        xsi:type="uml:Class"
        xmi:id="ContactUpdate"
        name="ContactUpdate">
      <generalization
          general="ContactProcessing"
          isSubstitutable="false"/>
    </packagedElement>
    <packagedElement
        xsi:type="uml:Class"
        xmi:id="UpdateConfirmation"
        name="UpdateConfirmation">
      <generalization
          general="Confirmation"
          isSubstitutable="false"/>
      <ownedAttribute
          name="contactUpdate"
          visibility="private"
          type="ContactUpdate"/>
      <ownedOperation
          name="updateContact"/>
    </packagedElement>
    <packagedElement
        xsi:type="uml:Class"
        xmi:id="ContactDeletion"
        name="ContactDeletion">
      <generalization
          general="ContactProcessing"
          isSubstitutable="false"/>
      <ownedAttribute
          name="contact"
          visibility="private"
          type="String"/>
    </packagedElement>
    <packagedElement
        xsi:type="uml:Class"
        xmi:id="DeleteConfirmation"
        name="DeleteConfirmation">
      <generalization
          general="Confirmation"
          isSubstitutable="false"/>
      <ownedAttribute
          name="contactDeletion"
          visibility="private"
          type="ContactDeletion"/>
      <ownedOperation
          name="deleteContact"/>
    </packagedElement>
    <packagedElement
        xsi:type="uml:Class"
        xmi:id="ContactDataInput"
        name="ContactDataInput">
      <ownedAttribute
          name="name"
          visibility="private"
          type="String"/>
      <ownedAttribute
          name="email"
          visibility="private"
          type="String"/>
      <ownedAttribute
          name="phoneMain"
          visibility="private"
          type="Phone"/>
      <ownedAttribute
          name="phoneAlternative"
          visibility="private"
          type="Phone"/>
      <ownedAttribute
          name="postalAddrMain"
          visibility="private"
          type="Address"/>
      <ownedAttribute
          name="postalAddrAlternative"
          visibility="private"
          type="Address"/>
      <ownedOperation
          name="validateData"/>
    </packagedElement>
    <packagedElement
        xsi:type="uml:Class"
        xmi:id="ValidationError"
        name="ValidationError">
      <ownedAttribute
          name="message"
          visibility="private"
          type="String"/>
      <ownedAttribute
          name="contactDataInput"
          visibility="private"
          type="ContactDataInput"/>
    </packagedElement>
  </uml:Package>
  <uml:Package
      xmi:id="content"
      name="content">
    <packagedElement
        xsi:type="uml:Class"
        xmi:id="AddressBook"
        name="AddressBook">
      <ownedAttribute
          name="introduction"
          visibility="private"
          type="String"/>
      <ownedAttribute
          name="contacts"
          visibility="private"
          type="Contact"
          isOrdered="true"
          isDerived="true">
        <lowerValue
            xsi:type="uml:LiteralInteger"/>
        <upperValue
            xsi:type="uml:LiteralUnlimitedNatural"
            value="10"/>
      </ownedAttribute>
    </packagedElement>
    <packagedElement
        xsi:type="uml:Class"
        xmi:id="Contact"
        name="Contact">
      <ownedAttribute
          name="name"
          visibility="private"
          type="String"/>
      <ownedAttribute
          name="email"
          visibility="private"
          type="String"/>
      <ownedAttribute
          name="addressBook"
          visibility="private"
          type="AddressBook"/>
      <ownedAttribute
          name="postalAddrMain"
          visibility="private"
          type="Address"/>
      <ownedAttribute
          name="postalAddrAlternative"
          visibility="private"
          type="Address"/>
      <ownedAttribute
          name="phoneMain"
          visibility="private"
          type="Phone"/>
      <ownedAttribute
          name="phoneAlternative"
          visibility="private"
          type="Phone"/>
      <ownedAttribute
          name="picture"
          visibility="private"
          type="Picture"/>
    </packagedElement>
    <packagedElement
        xsi:type="uml:Class"
        xmi:id="Address"
        name="Address">
      <ownedAttribute
          name="name"
          visibility="private"
          type="String"/>
      <ownedAttribute
          name="postalCode"
          visibility="private"
          type="Integer"/>
      <ownedAttribute
          name="city"
          visibility="private"
          type="String"/>
      <ownedAttribute
          name="country"
          visibility="private"
          type="String"/>
    </packagedElement>
    <packagedElement
        xsi:type="uml:Class"
        xmi:id="Phone"
        name="Phone">
      <ownedAttribute
          name="internationalAreaCode"
          visibility="private"
          type="Integer"/>
      <ownedAttribute
          name="prefix"
          visibility="private"
          type="Integer"/>
      <ownedAttribute
          name="number"
          visibility="private"
          type="Integer"/>
    </packagedElement>
    <packagedElement
        xsi:type="uml:Class"
        xmi:id="Picture"
        name="Picture">
      <ownedAttribute
          name="width"
          visibility="private"
          type="Integer"/>
      <ownedAttribute
          name="height"
          visibility="private"
          type="Integer"/>
      <ownedAttribute
          name="contact"
          visibility="private"
          type="Contact"/>
    </packagedElement>
    <packagedElement
        xsi:type="uml:PrimitiveType"
        xmi:id="String"
        name="java.lang.String"/>
    <packagedElement
        xsi:type="uml:PrimitiveType"
        xmi:id="Integer"
        name="int"/>
  </uml:Package>
</xmi:XMI>
